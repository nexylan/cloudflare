{
    "name": "nexylan/cloudflare",
    "description": "A PHP sdk to communicate with CloudFlare api V4",
    "keywords": ["CloudFlare", "API", "SDK"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "sullivan@nexylan.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/inflector": "^1.0",
        "php-http/client-common": "^1.7 || ^2.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.4",
        "symfony/options-resolver": "^2.7 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.1",
        "php-http/message": "^1.6",
        "php-http/httplug-bundle": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Nexy\\CloudFlare\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Nexy\\CloudFlare\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
